package codepractice;

public class Test2 {
	public static void main(String [] args){

		final class Constants{public final static String name="Pi";} // if we remove final it will throw compiler error
		
		Thread thread = new Thread(new Runnable(){
			@Override
			public void run(){
				System.out.println(Constants.name);
			}
		});
		
		thread.start();
	}	

}
