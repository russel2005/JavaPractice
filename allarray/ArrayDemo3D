package allarray;

public class ArrayDemo3D {

	public static void main(String[] args) {
		
		int [][][] test3D = {
				{	
					{1,2,3},
					{4,5}
				},
				{
					{6,7},
					{8}
				}
		};
		
		for (int[][] array2D: test3D) {			
	        for (int[] array1D: array2D) {
	           for(int item: array1D) {
	              System.out.print(item+ " ");
	           }System.out.println();
	        }System.out.println();
	     }
	}

}
