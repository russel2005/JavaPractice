public class Sample01 {

	public static void main(String[] args) {
		
		int num = 4;
		newNumber(5);
		System.out.println(num);
		
		String name = "Webby";
		speak(name);
		System.out.println(name);
	}
	public static void newNumber(int num){
		num = 8;
	}
	public static void speak(String name){
		name = "Sparky";
	}

}
